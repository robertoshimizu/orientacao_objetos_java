
public class Aluno {
	
	int bim1;
	int bim2;
	int bim3;
	int bim4;
	
	public int media() {
		int total = 0;
		total += a.bim1;
		total += a.bim2;
		total += a.bim3;
		total += a.bim4;
		return total / 4;
	}
	
	public boolean passouDeAno() {
		
		if(media() >= 60) {
			return true;		
		}
		return false;
	}


}
